{
  "name": "artifact",
  "version": "0.1.0",
  "private": true,
  "author": "Rafael R. Camargo (https://github.com/rafaelrcamargo)",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --ignore-unknown --write .",
    "postinstall": "simple-git-hooks",
    "lint": "next lint",
    "start": "next start"
  },
  "simple-git-hooks": {
    "pre-commit": "bun lint-staged --quiet"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "prettier": {
    "arrowParens": "avoid",
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "semi": false,
    "singleQuote": false,
    "trailingComma": "none"
  },
  "dependencies": {
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "next": "14.0.3-canary.2",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@studio-freight/react-lenis": "^0.0.36",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cobe": "^0.6.3",
    "eslint": "^8",
    "eslint-config-next": "14.0.3-canary.2",
    "framer-motion": "^10.16.4",
    "geist": "^1.1.0",
    "gsap": "^3.12.2",
    "lint-staged": "^14.0.0",
    "lucide-react": "^0.292.0",
    "next-themes": "^0.2.1",
    "postcss": "^8",
    "prettier": "3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "react-spring": "^9.7.3",
    "simple-git-hooks": "^2.9.0",
    "sonner": "^1.2.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5"
  }
}
